ivp_dependent: true
datastructs:
  - double Yprev[s][n]
  - double tmp
  - double b[s]
computations:
  A: "tmp += b[i] * %RHS"
  B: "y_new = tmp"
variants:
  - name : ys_RHS_Approx_ji
    code: |
      %LOOP_START i s unroll 0 assign 0
        %COMP A
      %LOOP_END i
      %COMP B
    pmodel :
      - working sets: { "(s+1)*n+s" }
codegen:
  RHS_input: "Yprev[i]"
  RHS_butcher_nodes: "c[i]"
model_tool: yasksite
